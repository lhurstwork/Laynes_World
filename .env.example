# Layne's World Dashboard - Environment Configuration Template
# Copy this file to .env.local for development or .env.production for production

# API Base URLs
VITE_NEWS_API_URL=https://api.example.com/news
VITE_YOUTUBE_API_URL=https://www.googleapis.com/youtube/v3
VITE_DEALS_API_URL=https://api.example.com/deals
VITE_GOOGLE_CALENDAR_API_URL=https://www.googleapis.com/calendar/v3
VITE_OUTLOOK_CALENDAR_API_URL=https://graph.microsoft.com/v1.0

# API Keys
VITE_NEWS_API_KEY=your_news_api_key_here
VITE_YOUTUBE_API_KEY=your_youtube_api_key_here
VITE_DEALS_API_KEY=your_deals_api_key_here

# OAuth Client IDs
VITE_GOOGLE_CLIENT_ID=your_google_oauth_client_id_here
VITE_OUTLOOK_CLIENT_ID=your_outlook_oauth_client_id_here

# OAuth Redirect URIs
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/google/callback
VITE_OUTLOOK_REDIRECT_URI=http://localhost:5173/auth/outlook/callback

# Environment Mode (development or production)
VITE_APP_ENV=development

# Mock Data Toggle (true for development, false for production)
VITE_USE_MOCK_DATA=true

# API Timeout (milliseconds)
VITE_API_TIMEOUT=10000

# Retry Configuration
VITE_MAX_RETRIES=3
VITE_RETRY_BASE_DELAY=1000

# Firebase Authentication Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
